Description='A simple WEP encrypted wireless connection'
Interface=wlan0
Connection=wireless
Security=wep
ESSID='MyNetwork'
# Prepend \" to hexadecimal keys
Key=\"1234567890abcdef
IP=dhcp
# Uncomment this if your ssid is hidden
#Hidden=yes
# Uncomment if you are using an ad-hoc connection
#AdHoc=yes
